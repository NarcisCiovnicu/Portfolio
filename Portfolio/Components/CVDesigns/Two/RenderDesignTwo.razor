@using Portfolio.Components.CVDesigns.One
@inherits BaseRenderDesign

<RenderStartSection CurriculumVitae="CurriculumVitae"></RenderStartSection>

@if (CurriculumVitae.EducationHistory != null)
{
    <RenderEducation EducationHistory="CurriculumVitae.EducationHistory"></RenderEducation>
}

@if (CurriculumVitae.WorkExperienceList != null)
{
    <RenderWorkExperience WorkExperienceList="@CurriculumVitae.WorkExperienceList"></RenderWorkExperience>
}

@if (CurriculumVitae.PersonalProjects != null)
{
    <RenderProjects PersonalProjects="CurriculumVitae.PersonalProjects"></RenderProjects>
}

@if (CurriculumVitae.Skills != null)
{
    <RenderSkills Skills="CurriculumVitae.Skills"></RenderSkills>
}
